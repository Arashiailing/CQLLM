import py

/**
 * @name CWE-287: Improper Authentication
 * @description Detects potential improper authentication issues by identifying
 *              authentication-related functions that may not properly validate credentials.
 */
predicate is_authentication_function(String name) {
  name = "authenticate" or name = "login" or name = "check_password" or
         name = "validate_credentials" or name = "verify_user" or
         name = "login_user" or name = "authorize" or
         name = "confirm_identity" or name = "authenticate_user"
}

